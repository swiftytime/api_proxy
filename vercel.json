{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "https://bc.swiftybase.com/:path*",
        }
    ],
    "routes": [
        {
            "src": "/:path*",
            "transforms": [
                {
                    "type": "request.headers",
                    "op": "append",
                    "target": {
                        "key": "Access-Control-Allow-Origin"
                    },
                    "args": "https://app.swiftybase.com"
                },
                {
                    "type": "request.headers",
                    "op": "append",
                    "target": {
                        "key": "Access-Control-Allow-Methods"
                    },
                    "args": "GET, POST, PUT, DELETE, OPTIONS"
                }
            ]
        }
    ]
}